{
  "env": {
      "browser": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "rules": {
      "no-use-before-define": 0,
      "no-await-in-loop": 0,
      "import/prefer-default-export": 0,
      "no-alert": 0,
      "no-param-reassign": 0
  }
}